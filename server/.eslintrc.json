{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-console": 0,
    "quotes": ["error", "double"],
    "object-curly-spacing": 0,
    "max-len": 0,
    "require-jsdoc": 0,
    "linebreak-style": 0,
    "no-invalid-this": 0,
    "new-cap": 0,
    "comma-dangle": 0
  }
}
